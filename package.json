{
  "name": "eagle-core-auth",
  "version": "1.0.0",
  "private": true,
  "description": "Authentication rest api for Hasura GraphQL Engine.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "copyfiles --flat src/keys/*.pem dist/keys && copyfiles -u 2 src/views/**/* dist/views && tsc-watch --onSuccess \"node dist/index.js\"",
    "build": "tsc && copyfiles --flat src/views/*.hjs dist/views",
    "keys": "node src/lib/genKeys.js && copyfiles --flat src/keys/*.pem dist/keys"
  },
  "author": "Leonardo Vizcaya (Arpix)",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.409.0",
    "@hapi/joi": "^17.1.1",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "hogan.js": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.5",
    "passport": "^0.6.0",
    "passport-bitbucket-token": "^1.0.0",
    "passport-facebook-token": "^4.0.0",
    "passport-github-token": "^3.0.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.1",
    "pem-jwk": "^2.0.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.17",
    "@types/hapi__joi": "^17.1.10",
    "@types/hogan.js": "^3.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.5",
    "@types/nodemailer": "^6.4.9",
    "@types/passport": "^1.0.12",
    "@types/passport-facebook-token": "^0.4.35",
    "@types/passport-jwt": "^3.0.9",
    "@types/pg": "^8.10.2",
    "copyfiles": "^2.4.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.2.2"
  }
}
