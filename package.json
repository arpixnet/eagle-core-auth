{
  "name": "eagle-core-auth",
  "version": "1.0.0",
  "private": true,
  "description": "Authentication rest api for Hasura GraphQL Engine.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "copyfiles --flat src/keys/*.pem dist/keys && copyfiles --flat src/views/*.hjs dist/views && tsc-watch --onSuccess \"node dist/index.js\"",
    "build": "tsc && copyfiles --flat src/views/*.hjs dist/views",
    "keys": "node src/lib/genKeys.js && copyfiles --flat src/keys/*.pem dist/keys"
  },
  "author": "Leonardo Vizcaya (Arpix)",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "hogan.js": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-facebook-token": "^3.3.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.0.2",
    "rsa-pem-to-jwk": "^1.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^16.0.12",
    "@types/hogan.js": "^3.0.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/morgan": "^1.9.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.3",
    "@types/passport-facebook-token": "^0.4.34",
    "@types/passport-jwt": "^3.0.3",
    "@types/pg": "^7.14.3",
    "copyfiles": "^2.2.0",
    "tsc-watch": "^4.2.3",
    "typescript": "^3.8.3"
  }
}
