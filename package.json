{
  "name": "eagle-core-auth",
  "version": "1.0.0",
  "private": true,
  "description": "Authentication rest api for Hasura GraphQL Engine.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "copyfiles --flat src/keys/*.pem dist/keys && copyfiles -u 2 src/views/**/* dist/views && tsc-watch --onSuccess \"node dist/index.js\"",
    "build": "tsc && copyfiles --flat src/views/*.hjs dist/views",
    "keys": "node src/lib/genKeys.js && copyfiles --flat src/keys/*.pem dist/keys"
  },
  "author": "Leonardo Vizcaya (Arpix)",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "aws-sdk": "^2.1231.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "hogan.js": "^3.0.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.3",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.5",
    "passport": "^0.6.0",
    "passport-bitbucket-token": "^1.0.0",
    "passport-facebook-token": "^4.0.0",
    "passport-github-token": "^3.0.0",
    "passport-google-token": "^0.1.2",
    "passport-jwt": "^4.0.0",
    "pem-jwk": "^2.0.0",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/hapi__joi": "^17.1.8",
    "@types/hogan.js": "^3.0.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-facebook-token": "^0.4.35",
    "@types/passport-jwt": "^3.0.6",
    "@types/pg": "^8.6.5",
    "copyfiles": "^2.4.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.4"
  }
}
